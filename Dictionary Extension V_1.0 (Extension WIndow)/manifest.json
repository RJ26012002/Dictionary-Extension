{
    "manifest_version": 3,
    "name": "AI Dictionary",
    "version": "1.0",
    "description": "A simple dictionary extension",
    "action": {
        "default_icon": {
            "48": "icon.png"
        },
        "default_popup": "popup.html"
    },
    "host_permissions": [
        "http://localhost:5000/"
    ],
    "permissions": [
        "storage", "activeTab"
    ]
}